PYTH_CLUSTER=pythnet

# Hyperliquid testnet endpoint
HL_TESTNET_URL=https://api.hyperliquid-testnet.xyz

# MASTER WALLET (single builder / signer account)
# Use ONE wallet only; this is your HIP-3 builder + signer.
HL_MASTER_ADDRESS=<YOUR_MASTER_ADDRESS>
HL_MASTER_PRIVATE_KEY=<YOUR_MASTER_PRIVATE_KEY>
# Alias used by deploy-dex.py (same key, no API sub-account)
HL_API_PRIVATE_KEY=<YOUR_MASTER_PRIVATE_KEY>

# HIP-3 identifiers
HL_DEX_NAME=XAU
HL_COIN_SYMBOL=XAU-TEST
HL_ASSET_ID=
HL_ORACLE_ENDPOINT=/exchange
HL_PUBLISH_ENABLED=false

# Publishing settings
PUBLISH_INTERVAL_MS=3000
MIN_PUBLISH_INTERVAL_MS=10000
PRICE_EPSILON=0.01
STALE_THRESHOLD_MS=10000

# Single-wallet configuration: no API sub-accounts.
